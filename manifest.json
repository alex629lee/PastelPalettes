{
  "manifest_version": 2,
  "name": "Pink Twitter",
  "description": "A cute pink theme for Twitter in Chrome",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["main.css"]
    }
  ],
  "page_action": {
    "default_icon": "icon128.png",
    "default_title": "Pink Theme"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}